{
    "name": "ebrew-vscode",
    "publisher": "4984",
    "displayName": "ebrew-vscode",
    "description": "ebrew syntax for vscode",
    "version": "0.1.1",
    "main": "client/ebrew.js",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ebrew",
            "aliases": ["eb", "ebrew"],
            "extensions": [".eb", ".eh"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ebrew",
            "scopeName": "source.ebrew",
            "path": "./syntaxes/ebrew.tmLanguage.json"
        }]
    },
    "activationEvents": [
        "onLanguage:ebrew"
    ]
}
