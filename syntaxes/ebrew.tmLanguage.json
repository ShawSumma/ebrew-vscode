{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ebrew",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#chars"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#opers"
		},
		{
			"include": "#ents"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"identifier": {
			"patterns": [
				{
					"name": "variable.name.ebrew",
					"match": "[a-zA-Z\\-_][a-zA-Z0-9\\-_]*"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.ebrew",
					"match": "\\b(let|for|and|or|do|addr|if)\\b"
				}
			]
		},
		"constants": {
			"patterns": [
				{
					"name": "constant.numeric.ebrew",
					"match": "\\b[0-9]+\\b"
				}
			]
		},
		"comments": {
			"patterns": [{
				"name": "comment.block.ebrew",
				"begin": "#",
				"end": "#"
			}]
		},
		"chars": {
			"patterns": [
				{
					"name": "string.quoted.single.ebrew",
					"match": "'\\\\?.'?"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"name": "string.quoted.double.ebrew",
					"match": "\"(\\\\\"|[^\"])*\""
				}
			]
		}
	},
	"scopeName": "source.ebrew"
}
